{
  "messagebusType": 1, /*0=memory 1=kafka  2=redis 3 rabbitmq */
  "kafka": {
    "BootstrapServers": "192.168.111.132:9092,192.168.111.132:9093,192.168.111.132:9094", /* com */
    "BootstrapServersHOme": "192.168.72.132:9092,192.168.72.132:9093,192.168.72.132:9094", /* home */
    "TopicPrefix": "kafka-messagebus-",
    "DefaultConsumerThreadCount": 2,
    "DelayConsumerThreadCount": 1,

    "ManualCommitBatch": 10,
    "ManualCommitIntervalSecond": 0,
    "ProducerConfig": {
      /*  "TransactionalId": "TransactionalId", */
      "Acks": "leader", /*设置整数也可以 1=leader确认  0=none(不确认)  -1=all 全部确认*/
      "Partitioner": "ConsistentRandom", /*分区算法 默认ConsistentRandom*/
      "MessageSendMaxRetries": 2, /*重试 默认2*/
      "EnableIdempotence": false /*幂等性 默认为false, 设置为true时Acks必须设置为all*/
    },
    "ConsumerConfig": {
      "AutoOffsetReset": 0, /*0=Latest  1=Earliest  2=Error*/
      "CancellationDelayMaxMs": 100, /*Consume.Poll的等待时间，默认是100毫秒*/
      "EnableAutoCommit": false, /*true=自动提交   false=手动提交*/
      "GroupId": "kafka-group" /*这里配置默认分组，订阅时可以覆盖*/
    }
  },
  "redis-messagebus": {
    "ConnectionStringCom": "192.168.111.132:6379",
    "ConnectionString": "192.168.72.132:6379",
    "TopicPrefix": "redis:messagebus:",
    "DefaultConsumerThreadCount": 4,
    "MaxErrorReTryCount": 5,
    "RetryStrategy": [ 5, 5, 10, 10, 60 ]
  },
  "rabbitmq": {
    "HostName": "192.168.111.132", /*"192.168.111.132"   192.168.72.133*/
    "Port": 5672,
    "VirtualHost": "/",
    "UserName": "root",
    "Password": "root123",
    "TopicPrefix": "rabbitmq-messagebus-",
    "ConfirmSelect": false,
    "DefaultConsumerThreadCount": 4,
    "AutoAck": false,
    "ManualCommitBatch": 100,
    "MaxErrorReTryCount":5,
    "RetryStrategy": [ 0, 5, 10, 10, 10 ]
  }

}